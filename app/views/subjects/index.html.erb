<table>
	<%= flash[:notice] %>
	<h3> Subjects List</h3>
	<thead>
		<tr>
			<th>Name</th>
			<th>type</th>
			<th>Student</th>
			<th>Image</th>
			<th>Actions</th>
		</tr>
	</thead>
	<tbody>
		<% @subjects.each do |subject| %>
		<tr>
			<td><%= subject.name %></td>
			<td><%= subject.sub_type %></td>
			<td><%= subject.student.name %></td>
			<td></td>
			<td><%= link_to "Edit " ,edit_subject_path(subject) %>
				<%= link_to "show " ,new_subject_path %>
				<%= link_to "Delete " ,subject_path(subject), method: :delete, data:{confirm: 'are you sure this data eleted '} %>
		</tr>
		<% end %>
	</tbody>
	<%= link_to "create new subject",new_subject_path %>

</table>