<div class="about_section">
<%= form_with model: @student  do |f| %>
    <div>
      <%= f.label :first_name %><br>
      <%= f.text_field :first_name %>
       <% @student.errors.full_messages_for(:first_name).each do |message| %>
       <p> <%= message %>
       <% end %>
    </div>
    <div>
      <%= f.label :middle_name %><br>
      <%= f.text_field :middle_name %>
       <% @student.errors.full_messages_for(:middle_name).each do |message| %>
       <p> <%= message %>
       <% end %>
    </div>
      <div>
