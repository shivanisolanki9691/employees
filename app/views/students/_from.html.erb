      <%= f.text_field :personal_email %>
       <% @student.errors.full_messages_for(:personal_email).each do |message| %>
       <p> <%= message %>
       <% end %>
    </div>
    <div>
      <%= f.label :state%><br>
      <%= f.text_field :state %>
       <% @student.errors.full_messages_for(:state).each do |message| %>
       <p> <%= message %>
