       <% @student.errors.full_messages_for(:address_line_1).each do |message| %>
       <p> <%= message %>
       <% end %>
    </div>
    <div>
      <%= f.label :address_line_2 %><br>
      <%= f.text_field :address_line_2 %>
       <% @student.errors.full_messages_for(:address_line_2).each do |message| %>
